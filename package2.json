{
  "name": "jilltuckernursingservices",
  "version": "1.0.1",
  "description": "Jill Tucker Nursing Services",
  "main": "js/index.es6",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server ./js/index.es6",
    "build": "babel --presets es2015 dist/index.js -d es5",
    "startX": "webpack-dev-server ./js/[name].bundle.es6",
    "startNS": "node server.js",
    "buildX": "node_modules/.bin/webpack --watch",
    "buildXX": "babel --presets es2015 dist/index.js -d es5",
    "dev": "webpack-dev-server --content-base dist/"
  },
  "scriptsXXX": {
    "dev": "./node-modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "node server.js",
    "build": "babel --presets es2015 js/index.js -d es5"
  },
  "scriptsSingle": {
    "instruction": "run this during dev. npm start, webpack -p --w. change html script src",
    "start": "webpack-dev-server ./js/index.es6",
    "build": "rm -rf dist/* && webpack --display-error-details"
  },
  "scriptsMulti": {
    "instruction": "run this during dev. with multiple entry npm run dev, webpack -p --w. change html script src",
    "startX": "webpack-dev-server ./index_bundle.js",
    "build": "babel --presets es2015 js/index_bundle.js -d es5",
    "dev": "webpack-dev-server --content-base dist/"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "babel-cli": "^6.10.1"
  },
  "author": "john odee",
  "license": "ISC"
}
